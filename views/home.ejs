<%- include("partials/header") -%>
<h1>Home</h1>
<p><%= startingContent %></p>
<% postContent.forEach(function(post) { %>
    <h2><%= post.postTitle %></h2>
    <p><%= post.postText.substring(0, 100) + '...' %>
    <a href="/posts/<%= post._id %> ">Read More</a>
    </p>
<%}) %> 
<%- include("partials/footer") -%>